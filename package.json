{
  "name": "videyo",
  "version": "1.0.0",
  "description": "a form of youtube clone written in Vanilla JS and powered by NodeJS",
  "main": "index.js",
  "author": "Densitty",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "pug": "^3.0.2"
  },
  "scripts": {
    "build": "babel init.js -d build",
    "prod": "npm run build && nodemon build/init.js",
